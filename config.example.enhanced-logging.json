{
  "Providers": [
    {
      "name": "openrouter",
      "api_base_url": "https://openrouter.ai/api/v1/chat/completions",
      "api_key": "sk-xxx",
      "models": [
        "google/gemini-2.5-pro-preview",
        "anthropic/claude-sonnet-4",
        "anthropic/claude-3.5-sonnet",
        "anthropic/claude-3.7-sonnet:thinking"
      ],
      "transformer": {
        "use": ["openrouter"]
      }
    },
    {
      "name": "deepseek",
      "api_base_url": "https://api.deepseek.com/chat/completions",
      "api_key": "sk-xxx",
      "models": ["deepseek-chat", "deepseek-reasoner"],
      "transformer": {
        "use": ["deepseek"],
        "deepseek-chat": {
          "use": ["tooluse"]
        }
      }
    },
    {
      "name": "ollama",
      "api_base_url": "http://localhost:11434/v1/chat/completions",
      "api_key": "ollama",
      "models": ["qwen2.5-coder:latest"]
    }
  ],
  "Router": {
    "default": "deepseek,deepseek-chat",
    "background": "ollama,qwen2.5-coder:latest",
    "think": "deepseek,deepseek-reasoner",
    "longContext": "openrouter,google/gemini-2.5-pro-preview",
    "longContextThreshold": 60000,
    "webSearch": "gemini,gemini-2.5-flash"
  },
  "APIKEY": "your-secret-key",
  "HOST": "0.0.0.0",
  "API_TIMEOUT_MS": 600000,
  "Logging": {
    "enabled": true,
    "level": "info",
    "timestamp": true,
    "serviceName": "claude-code-router",
    "version": "1.0.0",
    "environment": "development",
    "streams": [
      {
        "name": "error-file",
        "type": "file",
        "level": "error",
        "path": "./logs/error.log",
        "rotation": {
          "size": "10M",
          "interval": "1d"
        },
        "autoEnd": true
      },
      {
        "name": "combined-file",
        "type": "file",
        "level": "info",
        "path": "./logs/combined.log",
        "rotation": {
          "size": "10M",
          "interval": "1d"
        },
        "autoEnd": true
      },
      {
        "name": "debug-file",
        "type": "file",
        "level": "debug",
        "path": "./logs/debug.log",
        "rotation": {
          "size": "5M",
          "interval": "1h"
        },
        "autoEnd": true
      },
      {
        "name": "console-error",
        "type": "console",
        "level": "error",
        "stderr": true,
        "autoEnd": true
      },
      {
        "name": "console-output",
        "type": "console",
        "level": "info",
        "stderr": false,
        "autoEnd": true
      }
    ],
    "enhancedFeatures": {
      "requestTracking": {
        "enabled": true,
        "maxActiveRequests": 1000,
        "maxContextAge": 1800000,
        "autoCleanup": true,
        "cleanupInterval": 300000
      },
      "streamTracking": {
        "enabled": true,
        "enableMetrics": true,
        "enableProgressTracking": true
      },
      "errorLogging": {
        "enabled": true,
        "enableStatistics": true,
        "enableTrendAnalysis": true,
        "maxHistorySize": 1000
      },
      "performanceMetrics": {
        "enabled": true,
        "collectionInterval": 60000
      }
    },
    "integration": {
      "enableNewLogging": true,
      "migrateExistingLogs": true,
      "preserveExistingBehavior": true,
      "enableEnhancedFeatures": true,
      "logDirectory": "./logs",
      "logLevel": "info"
    },
    "backwardCompatibility": {
      "enabled": true,
      "convertExistingConfig": true,
      "generateMigrationSuggestions": true
    }
  },
  "LoggingPresets": {
    "production": {
      "enabled": true,
      "level": "info",
      "streams": [
        {
          "name": "production-error",
          "type": "file",
          "level": "error",
          "path": "./logs/production/error.log",
          "rotation": {
            "size": "50M",
            "interval": "1d"
          }
        },
        {
          "name": "production-combined",
          "type": "file",
          "level": "info",
          "path": "./logs/production/combined.log",
          "rotation": {
            "size": "100M",
            "interval": "1d"
          }
        },
        {
          "name": "production-console",
          "type": "console",
          "level": "warn",
          "stderr": false
        }
      ],
      "enhancedFeatures": {
        "requestTracking": {
          "enabled": true,
          "maxActiveRequests": 2000,
          "maxContextAge": 3600000
        },
        "errorLogging": {
          "enabled": true,
          "enableStatistics": true,
          "maxHistorySize": 5000
        }
      }
    },
    "development": {
      "enabled": true,
      "level": "debug",
      "streams": [
        {
          "name": "dev-console",
          "type": "console",
          "level": "debug",
          "stderr": false
        },
        {
          "name": "dev-file",
          "type": "file",
          "level": "debug",
          "path": "./logs/development/debug.log",
          "rotation": {
            "size": "10M",
            "interval": "1h"
          }
        }
      ],
      "enhancedFeatures": {
        "requestTracking": {
          "enabled": true,
          "maxActiveRequests": 100,
          "maxContextAge": 600000
        },
        "streamTracking": {
          "enabled": true,
          "enableMetrics": true,
          "enableProgressTracking": true
        }
      }
    },
    "testing": {
      "enabled": true,
      "level": "debug",
      "streams": [
        {
          "name": "test-console",
          "type": "console",
          "level": "debug",
          "stderr": false
        }
      ],
      "enhancedFeatures": {
        "requestTracking": {
          "enabled": false
        },
        "streamTracking": {
          "enabled": false
        },
        "errorLogging": {
          "enabled": true,
          "enableStatistics": false,
          "maxHistorySize": 100
        }
      }
    }
  },
  "EnvironmentVariables": {
    "ENABLE_ENHANCED_LOGGING": "true",
    "LOG_LEVEL": "info",
    "LOG_DIRECTORY": "./logs",
    "LOG_REQUEST_TRACKING": "true",
    "LOG_STREAM_TRACKING": "true",
    "LOG_ERROR_ENHANCEMENT": "true",
    "LOG_RETENTION_DAYS": "30",
    "LOG_MAX_FILE_SIZE": "10M",
    "LOG_MAX_FILES": "10"
  }
}